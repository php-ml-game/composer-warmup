{
    "name": "php-ml-game/composer-warmup",
    "description": "Composer warmup for PHP ML games.",
    "keywords": [],
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "php-ai/php-ml": "^0",
        "symfony/options-resolver": "^1",
        "symfony/polyfill-php73": "^4"
